<script> 
window.onload = () => {
  const token = '<%= token %>'; 
  fetch('/dashboard',{
    headers: {
      authorization: 'bearer ' + token
    }
  })
  .then(response => {
    if (response.ok) {
      window.location.href = '/dashboard';
    } else {
      console.error('Failed to redirect to /dashboard');
    }
  })
  .catch(error => {
    console.error('Failed to fetch /dashboard:', error);
  });
}
</script>

